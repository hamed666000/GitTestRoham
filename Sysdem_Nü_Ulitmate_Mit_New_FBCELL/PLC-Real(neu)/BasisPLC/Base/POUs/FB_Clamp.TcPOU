    </Method>
    <Method Name="SetCloseClampEmpty" Id="{e1f9b516-b57e-40fd-9545-e308aeecc481}">
      <Declaration><![CDATA[METHOD SetCloseClampEmpty : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[m_csStateRequested := CLAMP_STATE.e_CLOSED;
m_bOpen := FALSE;
